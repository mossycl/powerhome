{
    "manifest_version": 2,
    "name": "PowerHome",
    "description": "Create your custom homepage",
    "version": "1.0",
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "55.0"
      }
    },
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides":{
        "newtab": "index.html"
    },
    "chrome_settings_overrides": {
        "homepage": "index.html"
    }
  
  }